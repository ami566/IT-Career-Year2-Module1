using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
namespace GRID
{
    public class AggressiveDriver : Driver
    {
        public AggressiveDriver(string name) : base(name, 2.7)
        {
           
        }
        public override double Speed
        {
            get { return base.Speed * 1.3; }
        }
    }
}
